{
  "name": "maskani-api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "undo-migrate": "./node_modules/.bin/sequelize db:migrate:undo",
    "migrate": "./node_modules/.bin/sequelize db:migrate",
    "unseed": "./node_modules/.bin/sequelize db:seed:undo:all",
    "seed": "yarn unseed && ./node_modules/.bin/sequelize db:seed:all",
    "start": "yarn migrate && nodemon ./app.js"
  },
  "dependencies": {
    "bcrypt": "^4.0.1",
    "cors": "^2.8.5",
    "crypto-js": "^4.0.0",
    "dotenv-safe": "^8.2.0",
    "express": "^4.17.1",
    "express-validator": "^6.4.0",
    "helmet": "^3.22.0",
    "jsonwebtoken": "^8.5.1",
    "mailgun-js": "^0.22.0",
    "morgan": "^1.10.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "pg": "^8.0.2",
    "request-ip": "^2.1.3",
    "sequelize": "^5.21.6",
    "sequelize-cli": "^5.5.1",
    "trim-request": "^1.0.6",
    "uuid": "^7.0.3"
  },
  "devDependencies": {
    "nodemon": "^2.0.3"
  }
}
